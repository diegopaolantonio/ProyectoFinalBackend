<br />
<h1>Bienvenido {{name}}</h1>
<p>email: {{email}}</p>
<p>Edad: {{age}}</p>
<p>Cart: {{cart}}</p>
<p>Role: {{role}}</p>
<a href="/api/v1/sessions/logout"><button>Logout</button></a>
<br />
<h2>Productos:</h2>
{{#each productsArray}}
  <div>
    <p>Id: {{this._id}}</p>
    <p>Title: {{this.title}}</p>
    <p>Category: {{this.category}}</p>
    <p>Status: {{this.status}}</p>
    <br />
    <a href="/api/v1/{{cart}}/product/{{this._id}}"><button>Agregar a
        carrito</button></a>
    <br />
    <br />
    <a href="/product/detail/{{this._id}}"><button>Detalle</button></a>
    <br />
  </div>
{{/each}}

<br />
<div>
  <p>prevPage: {{prevPage}}</p>
  <p>page: {{actualPage}} / {{totalPages}}</p>
  <p>nextPage: {{nextPage}}</p>

  {{#if hasPrevPage}}
    <a href="{{prevLink}}"><button>Prev page</button></a>
  {{/if}}

  {{#if hasNextPage}}
    <a href="{{nextLink}}"><button>Next page</button></a>
  {{/if}}
</div>